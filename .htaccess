# Redirect old .html URLs to new .php
RewriteEngine On

# Redirect index.html to index.php
RewriteRule ^index\.html$ index.php [R=301,L]

# Redirect login.html to login.php
RewriteRule ^login\.html$ login.php [R=301,L]

# Redirect report.html to report.php
RewriteRule ^report\.html$ report.php [R=301,L]

# Default index to index.php
DirectoryIndex index.php index.html
